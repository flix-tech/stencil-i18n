{
  "name": "@flix-tech/stencil-i18n",
  "version": "0.0.5",
  "description": "Static translations for stenciljs",
  "author": "FlixMobility Tech",
  "license": "MIT",
  "repository": "https://github.com/flix-tech/stencil-i18n",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "stencil",
    "translate",
    "i18n"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist && npm run lint",
    "prepublishOnly": "npm ci && npm run build",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c rollup.config.js",
    "version": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint && npm run build && jest",
    "test.watch": "jest --watch",
    "postversion": "git push origin && git push origin --tags"
  },
  "dependencies": {
    "acorn": "6.2.0",
    "acorn-walk": "6.2.0",
    "astring": "1.4.0"
  },
  "devDependencies": {
    "@types/astring": "1.3.0",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.2",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "rollup": "1.16.7",
    "rollup-plugin-node-resolve": "5.2.0",
    "tslint": "5.18.0",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  }
}
