{
  "name": "@flix-tech/stencil-i18n",
  "version": "0.0.6",
  "description": "Static translations for stenciljs",
  "author": "FlixMobility Tech",
  "license": "MIT",
  "repository": "https://github.com/flix-tech/stencil-i18n",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "stencil",
    "translate",
    "i18n"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist && npm run lint",
    "prepublishOnly": "npm ci && npm run build",
    "build": "tsc && npm run rollup",
    "watch": "tsc --watch",
    "rollup": "rollup -c rollup.config.js",
    "version": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run lint && npm run build && jest",
    "test.watch": "jest --watch",
    "postversion": "git push origin && git push origin --tags"
  },
  "dependencies": {
    "acorn": "7.2.0",
    "acorn-walk": "7.1.1",
    "astring": "1.4.3"
  },
  "devDependencies": {
    "@types/astring": "1.3.0",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.10",
    "jest": "26.0.1",
    "rimraf": "3.0.2",
    "rollup": "2.13.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "tslint": "6.1.2",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "3.9.3"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts|tsx)$": "<rootDir>/test/jest.preprocessor.js"
    },
    "testRegex": "(\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ],
    "testURL": "http://localhost"
  }
}
